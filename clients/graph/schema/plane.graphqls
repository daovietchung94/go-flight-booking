type Plane {
    id: ID!
    number: String!
    numOfSeats: Int!
    status: PlaneStatus!
}

input GetPlanesRequest {
    page: Int!
    limit: Int!
    sort: String!
}

type GetPlanesResponse {
    page: Int!
    limit: Int!
    sort: String!
    totalRows: Int!
    totalPages: Int!
    rows: [Plane!]
}

input CreatePlaneRequest {
    number: String!
    numOfSeats: Int!
    status: PlaneStatus!
}

enum PlaneStatus {
    CLEANING,
    REPAIRING,
    READY
}